{
  "expo": {
    "name": "CILTpro",
    "slug": "ciltpro",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#EBD9CD"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "deploymentTarget": "13.0",
      "flipper": true,
      "supportsTablet": true,
      "bundleIdentifier": "com.greenfields.ciltpro"
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/ciltproicon2.png",
        "backgroundColor": "#f1f1f1",
        "googleServiceFile": "process.env.GOOGLE_SERVICES_JSON"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBmLj_6tI-wVz3yrcNXg6qTi1VPAOmzMNY"
        }
      },
      "package": "com.greenfields.ciltpro"
    },
    "web": {
      "favicon": "./assets/ciltproicon2.png"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ],
      [
        "expo-screen-orientation",
        {
          "initialOrientation": "PORTRAIT"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "2fd04d08-786b-4fef-883b-ad00d09fe4b0"
      }
    },
    "owner": "raffyjo"
  }
}
